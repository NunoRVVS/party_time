<div class="jumbotron" style="width:900px; padding: 20px;">
  <h1 class="display-4">New Order</h1>
  <hr class="my-4">
  <%= simple_form_for [@party_item, @order] do |f| %>
  <%= f.input :start, as: :date, html5: true %>
  <%= f.input :end, as: :date, html5: true %>
  <%= f.submit "Submit Order", class: "btn btn-primary" %>
<% end %>
</div>

