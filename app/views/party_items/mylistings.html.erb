<div class="jumbotron" style="width:900px; padding: 20px;">
  <h1 class="display-4">Order</h1>
  <hr class="my-4">
  <ul>
  <% @orders.each do |order| %>
  <% if order.user == current_user %>
  <li><%= link_to "Order ##{order.id}", order_path(order) %> :: <%= order.party_item.name %> starting on: <%= order.start %> </li>
  <% end %>
  <% end %>
</ul>
</div>
